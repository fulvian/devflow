# DevFlow Orchestrator
ORCHESTRATOR_PORT=3005
DEVFLOW_API_SECRET=devflow-orchestrator-token

# Synthetic Provider Selection
DEVFLOW_SYNTHETIC_PROVIDER=local
DEVFLOW_PROVIDER_FALLBACK=synthetic,openrouter

# Batching (optional)
DEVFLOW_SYNTHETIC_BATCH=0
DEVFLOW_SYNTHETIC_BATCH_WINDOW_MS=120
DEVFLOW_SYNTHETIC_MAX_BATCH_SIZE=8

# External API keys (set in environment, not in repo)
SYNTHETIC_API_KEY=
SYNTHETIC_API_BASE_URL=https://api.synthetic.new/v1
OPENROUTER_API_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Database
DEVFLOW_DB_PATH=./data/devflow.sqlite

