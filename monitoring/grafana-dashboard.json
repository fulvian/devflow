{
  "dashboard": {
    "id": null,
    "title": "Cometa Brain Command Processing Dashboard",
    "tags": ["cometa", "brain", "command-processing"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Command Processing Rate",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "expr": "rate(cometa_brain_commands_processed_total[1m])",
            "legendFormat": "Commands/sec",
            "format": "time_series"
          }
        ],
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 2
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Command Success Rate",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "expr": "100 * sum(rate(cometa_brain_commands_processed_total{status=\"success\"}[1m])) / sum(rate(cometa_brain_commands_processed_total[1m]))",
            "legendFormat": "Success Rate",
            "format": "time_series"
          }
        ],
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 3,
        "type": "graph",
        "title": "Command Response Time",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "expr": "histogram_quantile(0.95, sum(rate(cometa_brain_command_processing_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "95th Percentile",
            "format": "time_series"
          },
          {
            "refId": "B",
            "expr": "histogram_quantile(0.50, sum(rate(cometa_brain_command_processing_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "Median",
            "format": "time_series"
          },
          {
            "refId": "C",
            "expr": "rate(cometa_brain_command_processing_duration_seconds_sum[1m]) / rate(cometa_brain_command_processing_duration_seconds_count[1m])",
            "legendFormat": "Average",
            "format": "time_series"
          }
        ],
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3
          }
        },
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Active Tasks",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "expr": "cometa_brain_active_tasks",
            "legendFormat": "Active Tasks",
            "format": "time_series"
          }
        ],
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "decimals": 0
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      }
    ],
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [],
    "liveNow": false,
    "weekStart": ""
  }
}