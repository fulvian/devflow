{
  "project": {
    "name": "real-world-test",
    "version": "1.0.0",
    "description": "Real-world test project for DevFlow cognitive task and memory system validation",
    "author": "DevFlow Team",
    "createdAt": "2024-01-15T00:00:00Z"
  },
  "devflow": {
    "version": "2.1.0",
    "mode": "testing",
    "taskDirectory": ".devflow/tasks",
    "memoryDirectory": ".devflow/memory",
    "syncInterval": 30000,
    "semanticAnalysis": true,
    "cognitiveMemory": {
      "enabled": true,
      "vectorDimensions": 384,
      "similarityThreshold": 0.7
    }
  },
  "integrations": {
    "git": {
      "enabled": true,
      "autoCommit": false
    },
    "ci": {
      "enabled": true,
      "provider": "github-actions"
    },
    "synthetic": {
      "enabled": true,
      "apiKey": "${SYNTHETIC_API_KEY}"
    }
  },
  "testing": {
    "performanceThresholds": {
      "taskCreation": 100,
      "hierarchyQuery": 200,
      "memoryQuery": 150
    }
  }
}