# DevFlow Real-World Testing Environment Configuration

# Synthetic API Configuration
SYNTHETIC_API_KEY=your_synthetic_api_key_here
SYNTHETIC_API_BASE_URL=https://api.synthetic.new
SYNTHETIC_API_TIMEOUT=30000
SYNTHETIC_API_RATE_LIMIT=135
SYNTHETIC_API_RATE_WINDOW=18000000

# Environment
NODE_ENV=development

# Database Configuration
DATABASE_PATH=./data/devflow-test.sqlite
DATABASE_MEMORY_CACHE=64MB

# DevFlow Configuration
DEVFLOW_TOKEN_BUDGET=2000
DEVFLOW_SIMILARITY_THRESHOLD=0.7
DEVFLOW_BATCH_SIZE=10

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/devflow-test.log